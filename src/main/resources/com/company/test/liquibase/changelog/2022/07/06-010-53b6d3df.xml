<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1" author="Test">
        <createTable tableName="CARS">
            <column name="ID" type="UUID">
                <constraints nullable="false" primaryKey="true" primaryKeyName="PK_CARS"/>
            </column>
            <column name="MARK" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="TYPE_" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="COST" type="DOUBLE">
                <constraints nullable="false"/>
            </column>
            <column name="RENT_COST" type="DOUBLE">
                <constraints nullable="false"/>
            </column>
            <column name="VERSION" type="INT">
                <constraints nullable="false"/>
            </column>
            <column name="CREATED_BY" type="VARCHAR(255)"/>
            <column name="CREATED_DATE" type="DATETIME"/>
            <column name="LAST_MODIFIED_BY" type="VARCHAR(255)"/>
            <column name="LAST_MODIFIED_DATE" type="DATETIME"/>
            <column name="DELETED_BY" type="VARCHAR(255)"/>
            <column name="DELETED_DATE" type="DATETIME"/>
        </createTable>
    </changeSet>
    <changeSet id="2" author="Test">
        <createTable tableName="CLIENTS">
            <column name="ID" type="UUID">
                <constraints nullable="false" primaryKey="true" primaryKeyName="PK_CLIENTS"/>
            </column>
            <column name="NAME" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="ADDRES" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="EMAIL" type="VARCHAR(255)"/>
            <column name="PHONE_NUMBER" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="LICENSE_DATE" type="DATE">
                <constraints nullable="false"/>
            </column>
            <column name="LICENSE_DATE_EXPIRATION" type="DATE">
                <constraints nullable="false"/>
            </column>
            <column name="VERSION" type="INT">
                <constraints nullable="false"/>
            </column>
            <column name="CREATED_BY" type="VARCHAR(255)"/>
            <column name="CREATED_DATE" type="DATETIME"/>
            <column name="LAST_MODIFIED_BY" type="VARCHAR(255)"/>
            <column name="LAST_MODIFIED_DATE" type="DATETIME"/>
            <column name="DELETED_BY" type="VARCHAR(255)"/>
            <column name="DELETED_DATE" type="DATETIME"/>
        </createTable>
    </changeSet>
</databaseChangeLog>